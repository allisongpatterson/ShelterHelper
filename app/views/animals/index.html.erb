<h2>Animal Index Page</h2>

<table>
  <thead>
    <th>ID</th>
    <th>Name</th>
    <th>Age</th>
    <th>Sex</th>
    <th>Shelter ID</th>
  </thead>
  <tbody>
    <%- @animals.each do |animal| -%>
      <tr>
        <td><%= animal.id %></td>
        <td><%= animal.name %></td>
        <td><%= animal.age %></td>
        <td><%= animal.sex %></td>
        <td><%= animal.shelter_id %></td>
      </tr>
    <%- end -%>
  </tbody>
</table>
