<h2>Create a Shelter Page</h2>

<%= form_for @shelter do |shelter_form| %>
  <%= shelter_form.label :name %>
  <%= shelter_form.text_field :name %>
  <%= shelter_form.label :city %>
  <%= shelter_form.text_field :city %>
  <%= shelter_form.label :phone %>
  <%= shelter_form.text_field :phone %>
  <%= shelter_form.submit 'submit', class: 'btn btn-primary' %>
<% end %>